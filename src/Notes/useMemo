function App() {
  const [count, setCount] = React.useState(0);
  const [number, setNumber] = React.useState(5);

  const expensiveResult = React.useMemo(() => {
    return slowFunction(number);
  }, [number]);

  return (
    <>
      <p>Kết quả: {expensiveResult}</p>
      <button onClick={() => setCount(count + 1)}>
        Click {count}
      </button>
    </>
  );
}
